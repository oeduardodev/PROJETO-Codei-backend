# Endereço e porta do servidor
HOST=127.0.0.1
PORT=3333

# Chave de criptografia da aplicação (gerar com: node ace generate:key)
APP_KEY=your_app_key_here

# Nome da aplicação
APP_NAME=YourAppName

# Disco padrão para uploads (local, s3, etc.)
DRIVE_DISK=local

# Ambiente da aplicação (development, production, etc.)
NODE_ENV=development

# Configuração do banco de dados (sqlite, mysql, postgres, etc.)
DB_CONNECTION=sqlite

# Cloudinary - serviço de armazenamento de mídia (gerar em sua conta na cloudinary)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret